[Configuration]
LastActiveMode=THUNAR_RENAMER_MODE_NAME
LastActiveRenamer=ThunarSbrNumberRenamer

[ThunarSbrCaseRenamer]
Mode=THUNAR_SBR_CASE_RENAMER_MODE_LOWER

[ThunarSbrReplaceRenamer]
Regexp=TRUE
Replacement=jpg
CaseSensitive=FALSE
Pattern=JPG

[ThunarSbrInsertRenamer]
Offset=0
Mode=THUNAR_SBR_INSERT_MODE_INSERT
OffsetMode=THUNAR_SBR_OFFSET_MODE_RIGHT
Text=.pdf

[ThunarSbrRemoveRenamer]
EndOffsetMode=THUNAR_SBR_OFFSET_MODE_RIGHT
StartOffset=4
EndOffset=0
StartOffsetMode=THUNAR_SBR_OFFSET_MODE_RIGHT

[ThunarSbrNumberRenamer]
TextMode=THUNAR_SBR_TEXT_MODE_TN
Mode=THUNAR_SBR_NUMBER_MODE_010203
Start=01
Text=dino_

